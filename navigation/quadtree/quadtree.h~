#ifndef QUADTREE_H
#define QUADTREE_H

template <typename T>
class QuadTree {
	private:
		struct treeNode {
			T data;
			treeNode * outerLeft;
			treeNode * innerLeft;
			treeNode * innerRight;
			treeNode * outerRight;
		};
		treeNode * root;
	public:
		QuadTree()
		{
			root = new treeNode;
		}
		~QuadTree()
		{
			delete root;
		}
		void insert(T data);
		void remove(T data);
		T getMax();
};

template <typename T>
void QuadTree<T>::insert(T data)
{
	root->data = data;
}

template <typename T>
void QuadTree<T>::remove(T data)
{
}

template <typename T>
T QuadTree<T>::getMax()
{
	return root->data;
}

#endif
